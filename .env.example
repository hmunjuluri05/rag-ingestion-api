# Azure Document Intelligence Configuration
# Create an Azure Document Intelligence resource and get your endpoint and key
AZURE_DOCUMENT_INTELLIGENCE_ENDPOINT=https://your-resource.cognitiveservices.azure.com/
AZURE_DOCUMENT_INTELLIGENCE_KEY=your-api-key-here

# Azure Blob Storage (for production RAG pipeline)
AZURE_STORAGE_ACCOUNT=your-storage-account
AZURE_STORAGE_KEY=your-storage-key

# GCP Storage (for production RAG pipeline)
GCP_PROJECT_ID=your-project-id
GCP_STORAGE_BUCKET=your-bucket-name

# Redis/Celery Configuration (for production RAG pipeline)
REDIS_HOST=localhost
REDIS_PORT=6379
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# API Configuration (optional)
API_HOST=0.0.0.0
API_PORT=8000
